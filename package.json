{
  "name": "next-pandacss-typescript-boilerplate",
  "version": "0.0.0",
  "description": "A next boilerplate with Panda CSS for styling and batteries included setup from tests to releases.",
  "scripts": {
    "build": "next build",
    "ci": "next lint && tsc --noEmit",
    "codegen": "panda codegen --clean",
    "commit": "cz",
    "dev": "rm -rf .next && next dev",
    "export": "next export",
    "lint-staged": "lint-staged",
    "lint": "next lint",
    "types": "tsc --noEmit",
    "postbuild": "next-sitemap --config next-sitemap-config.js",
    "prepare": "husky install && panda codegen --clean",
    "release:patch": "standard-version --no-verify --release-as patch",
    "release": "standard-version --no-verify",
    "start": "next start",
    "test:e2e": "playwright test",
    "test": "jest"
  },
  "keywords": [
    "next",
    "typescript"
  ],
  "author": "Franrey Anthony S. Saycon",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-collapsible": "1.0.3",
    "@radix-ui/react-icons": "1.1.1",
    "camelcase-keys": "8.0.2",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@commitlint/cz-commitlint": "17.2.0",
    "@pandacss/dev": "0.18.3",
    "@playwright/test": "1.27.0",
    "@testing-library/jest-dom": "6.4.1",
    "@testing-library/react": "14.2.1",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "18.7.20",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "commitizen": "4.2.5",
    "eslint": "8.24.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-n": "15.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-react": "7.27.0",
    "husky": "8.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "lint-staged": "13.0.3",
    "msw": "0.47.3",
    "next-sitemap": "3.1.22",
    "prettier": "2.4.1",
    "standard-version": "9.5.0",
    "typescript": "4.8.3"
  }
}
